Interface DiscountCalculator {
(price:number):number
}
const discount:DiscountCalculator=(p)=>p.0.5

interface TechMachine{
start():void
stop():void
}

interface rating {
[name:string]:number
}
const chaiRating:rating{
masala:4.5
ginger:3.4
}

interface user{ name:string }
